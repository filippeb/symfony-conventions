{
    "name": "phpro/conventions",
    "description": "Common symfony rules for GrumPHP",
    "type": "composer-plugin",
    "license": "propriatary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "composer-plugin-api": "^2.1",
        "brianium/paratest": "*",
        "nikic/php-parser": "^4.3",
        "php-cs-fixer/shim": "^3.6",
        "phpro/grumphp-shim": "^1.5",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.4",
        "phpunit/phpunit": "*",
        "slowprog/composer-copy-file": "^0.3.3",
        "symplify/phpstan-rules": "^10.1"
    },
    "require-dev": {
        "composer/composer": "^2.2",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "PHProConventions\\": "src"
        }
    },
    "suggest": {
        "roave/security-advisories": "Required to run the security check"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "class": "PHProConventions\\Composer\\PhproConventionsPlugin"
    }
}
